pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
    }
    post {
        success{
            build job: 'CD',
            parameters: [
                service: 'test'
            ]
        }
    }
}
